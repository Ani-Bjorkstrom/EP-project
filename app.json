{
  "expo": {
    "name": "ImageShare",
    "slug": "ImageShare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF",
    
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true,
        "NSLocationWhenInUseUsageDescription": "This app uses your current location",
        "NSLocationAlwaysUsageDescription": "This app uses your current location",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your current location",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ],
        "Permissions.LOCATION": "Always"
      }
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
        
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}